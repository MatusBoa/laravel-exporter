{
    "name": "matusboa/laravel-exporter",
    "description": "Prometheus Exporter for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Matúš Koterba"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matusboa\\LaravelExporter\\": "src/"
        }
    },
    "require": {
        "php": "^8.3",
        "illuminate/mail": "^10.0|^11.0",
        "illuminate/queue": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "promphp/prometheus_client_php": "^2.12"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.65",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "code:fix": "vendor/bin/php-cs-fixer fix --using-cache no"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Matusboa\\LaravelExporter\\LaravelExporterServiceProvider"
            ]
        }
    }
}
